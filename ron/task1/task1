import java.util.Scanner;
class Human{
    private String gender;
    private String name;
    private int height;
    private int age;
    private String eyes_color;
    private String hair_color;

    Human(String gender, String name, int height, int age, String eyes_color, String hair_color) {
        this.gender = gender;
        this.height = height;
        this.name = name;
        this.eyes_color = eyes_color;
        this.hair_color = hair_color;
    }
        Human(String gender, String name, int age) {
            this.gender = gender;
            this.name = name;
            this.age = age;
        }

        Human(){}

    public void getInfo() {
            System.out.println("Object human. Gender: " + gender);
            System.out.println("Height: " + height);
            System.out.println("Name: " + name);
            System.out.println("Eyes color: " + eyes_color);
            System.out.println("Hair color: " + hair_color);
            System.out.println("Age: " + age);
    }
    }
public class Main {
                public static void main(String[] args) {
                    Scanner in = new Scanner(System.in);
                    Human human = new Human("man","Ilya", 24);
                   human.getInfo();
                }
            }
